 def insert(stack, temp):
      if not stack or stack[-1] <= temp:
         stack.append(temp)
         return
   
     val = stack.pop()
     insert(stack, temp)
      stack.append(val)

  def sortStack(stack):
     if stack:
         temp = stack.pop()
        sortStack(stack)
        insert(stack, temp)

 stack = [4, 1, 3, 2]
 sortStack(stack)
