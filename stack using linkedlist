class Node():
    def __init__(self, x):
        self.data = x
        self.next = None

class StackList():
    def __init__(self):
        self.top = None

    def push(self, x):
        newNode = Node(x)
        newNode.next = self.top
        self.top = newNode

    def pop(self):
        if self.isEmpty():
            print("Stack Underflow")
            return None
        temp = self.top.data
        self.top = self.top.next
        return temp

    def isEmpty(self):
        return self.top is None
